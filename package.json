{
  "name": "create-any-cli",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "Simple but highly flexible scaffolding CLI and Node API.",
  "bin": {
    "create": "./.bin/create"
  },
  "main": "./dist/api.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js",
    "clean": "rimraf dist",
    "build": "NODE_ENV=production babel src --out-dir dist --ignore spec.js && flow-copy-source -v src dist --ignore='*.spec.js'",
    "prebuild": "yarn clean",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-typed-install": "flow-typed install --ignoreDeps=bundled peer --overwrite",
    "prepare": "yarn flow-typed-install || true",
    "prepublishOnly": "yarn build",
    "release": "semantic-release"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "keywords": [
    "create",
    "cli",
    "create-",
    "create-*",
    "any",
    "kickstart",
    "bootstrap",
    "blueprint"
  ],
  "devDependencies": {
    "@immowelt/babel-preset-immowelt-node": "1.1.1",
    "@immowelt/eslint-config-immowelt-react": "2.1.1",
    "@immowelt/jest-preset-node": "1.1.1",
    "babel-cli": "6.26.0",
    "eslint": "5.6.1",
    "flow-bin": "0.83.0",
    "flow-copy-source": "2.0.2",
    "flow-typed": "2.5.1",
    "jest": "23.6.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "semantic-release": "13.4.1"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        }
      ]
    },
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/create-any-cli.git"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "dot": "1.1.2",
    "find-config-up": "1.1.1",
    "fs-extra": "7.0.0",
    "glob": "7.1.3",
    "inquirer": "6.2.0",
    "lodash": "4.17.11",
    "meow": "5.0.0",
    "ora": "3.0.0"
  }
}
